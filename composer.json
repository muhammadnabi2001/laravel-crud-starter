{
    "name": "muhammadnabi2001/laravel-crud-starter",
    "description": "Custom CRUD generator for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "muhammadnabi2001/laravel-crud-starter",
            "email": "xoliqulovmuhammadnabi842@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "CrudStarter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CrudStarter\\CrudStarterServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/muhammadnabi2001/laravel-crud-starter.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
